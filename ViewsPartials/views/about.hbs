    {{>links}}

    <title>About US</title>

</head>
<body>

    {{>navbar}}

    <div class="about">
        <div class="container-fluid main_cont" style="min-height: 100vh;padding: 0px;margin-top: -32px;">
            <div class="row">
                <div class="col-md-10 col-12 mx-auto">
                    <div class="row">
    
                        <div class="col-md-6 col-12 main_cont_right">
                            <figure>
                                <img id="fig" src="images/about.jpg" alt="" style="width: 100%;object-fit: contain;border-radius: 10px;">
